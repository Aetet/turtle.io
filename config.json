{
	"debug"   : false,
	"port"    : 8000,
	"compress": true,
	"headers" : {
		"Cache-Control"                : "max-age=3600 must-revalidate",
		"Access-Control-Allow-Origin"  : "*"
	},
	"index"   : ["index.htm", "index.html"],
	"logs"    : {
		"dir"    : "./logs",
		"date"   : "YYYYMMDD",
		"daemon" : "turtle_{{date}}.log",
		"error"  : "error_{{date}}.log",
		"format" : "{{host}} [{{time}}] \"{{method}} {{path}} HTTP/1.1\" {{status}} \"{{user-agent}}\""
	},
	"root"    : "./sites",
	"vhosts"  : {
		"localhost" : "test",
		"test"      : "test",
		"test2"     : "test2"
	},
	"tmp"     : "/tmp",
	"auth"    : {
		"test2" : {
			"authRealm" : "Private",
			"authList"  : ["admin:admin"]
		}
	}
}