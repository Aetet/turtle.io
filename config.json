{
	"auth"    : {},
	"compress": true,
	"default" : null,
	"probes"  : true,
	"maxConnections" : 25,
	"ps"      : 2,
	"uid"     : null,
	"cache"   : 1000,
	"headers" : {
		"Access-Control-Allow-Origin" : "*",
		"Access-Control-Allow-Headers": "*",
		"Access-Control-Allow-Methods": "",
		"Allow"                       : "",
		"Cache-Control"               : "public, max-age=0",
		"Content-Type"                : "text/html; charset=utf-8",
		"Date"                        : "",
		"Last-Modified"               : ""
	},
	"index"   : ["index.htm", "index.html"],
	"lag"     : 50,
	"logs"    : {
		"file"   : "turtleio.log",
		"time"   : "D/MMM/YYYY:HH:mm:ss ZZ",
		"format" : "{{host}} - {{user}} [{{time}}] \"{{method}} {{path}} HTTP/1.1\" {{status}} {{length}} {{referer}} \"{{user-agent}}\"",
		"flush"  : 1000,
		"stdout" : true
	},
	"queue"   : {
		"id"     : 1,
		"status" : true,
		"size"   : 200,
		"time"   : 1000,
		"handler": null
	},
	"port"    : 8000,
	"root"    : "",
	"routesHash" : {},
	"session" : {
		"id"     : "sid",
		"salt"   : "turtleio",
		"valid"  : "15 minutes",
		"gc"     : 60000
	},
	"pages"   : null,
	"proxy"   : {
		"rewrite" : [
			"text/html",
			"text/plain",
			"application/json"
		]
	},
	"tmp"     : "/tmp",
	"vhosts"  : {}
}