{
	"port"    : 8000,
	"compress": true,
	"headers" : {
		"Accept"                      : "text/html, text/plain",
		"Access-Control-Allow-Origin" : "*",
		"Access-Control-Allow-Headers": "Accept, Allow, Cache-Control, Content-Length, Content-Type, Date, Etag, Transfer-Encoding, Server",
		"Access-Control-Allow-Methods": "",
		"Allow"                       : "",
		"Cache-Control"               : "max-age=3600 must-revalidate",
		"Content-Type"                : "text/html; charset=utf-8",
		"Date"                        : "",
		"Last-Modified"               : ""
	},
	"index"   : ["index.htm", "index.html"],
	"logs"    : {
		"file"   : "turtle_io.log",
		"format" : "{{host}} [{{time}}] \"{{method}} {{path}} HTTP/1.1\" {{status}} \"{{user-agent}}\""
	},
	"root"    : "./sites",
	"vhosts"  : {
		"localhost" : "test",
		"test"      : "test",
		"test2"     : "test2"
	},
	"tmp"     : "/tmp",
	"auth"    : {
		"test2" : {
			"authRealm" : "Private",
			"authList"  : ["admin:admin"]
		}
	}
}